@using ProjetoPv.Areas.Identity.Data;
@model IEnumerable<ProjetoPvUser>
@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>Edit</h1>

<h3>User Roles</h3>

    <div class="form-group">
    @foreach (var item in Model)
    {
        <div class="form-group">
            <label asp-for="@item.Email" class="control-label"></label>
            <select asp-for="@item.Email" class="form-control" asp-items="ViewBag.Id"></select>
        </div>
        
    }
    </div>
<div class="form-group">
    <input type="submit" value="Save" class="btn btn-primary" />
</div>